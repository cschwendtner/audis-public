    <members>
        <member name="T:Audis.Primitives.CaseInsensitiveValueOfString`1">
            <summary>
            A <see cref="T:Audis.Primitives.ValueOf`2"/> where the underlying type is a <see cref="T:System.String"/>.
            The comparision with other strings is case-insensitive.
            Values must not be null.
            </summary>
        </member>
        <member name="T:Audis.Primitives.QuestionId">
            <summary>
            Identifies a question with <see cref="P:Audis.Primitives.QuestionId.ConfigurationName"/> and <see cref="P:Audis.Primitives.QuestionId.LineNumber"/> ("questionConfigurationName:LineNumber").
            </summary>
        </member>
        <member name="T:Audis.Primitives.AnswerId">
            <summary>
            Identifies an answer with <see cref="P:Audis.Primitives.AnswerId.QuestionId" /> and <see cref="P:Audis.Primitives.AnswerId.LineNumber"/> ("questionConfigurationName:questionLineNumber/answerLineNumber").
            </summary>
        </member>
        <member name="T:Audis.Primitives.ValueOf`2">
            <summary>
            Base class for wrapping primitive types.
            Used to prevent primitive obsession, see https://refactoring.guru/smells/primitive-obsession.
            </summary>
            <typeparam name="TValue">The primitive type to wrap.</typeparam>
            <typeparam name="TThis">The wrapping type itself.</typeparam>
        </member>
        <member name="T:Audis.Primitives.ValueOfTypeConverter`2">
            <summary>
            Type converter for <see cref="T:Audis.Primitives.ValueOf`2"/> types to allow seamingless serialization and deserialization.
            Use this type as parameter for the <see cref="T:System.ComponentModel.TypeConverterAttribute"/> for subclasses of <see cref="T:Audis.Primitives.ValueOf`2"/>.
            </summary>
        </member>
    </members>
